if not game:IsLoaded() then
    game.Loaded:Wait()
end

local Item = "Gold"
local Amount = game:GetService("Players").LocalPlayer.PlayerGui.MainGui.RightPanel.Inventory.List["Gold"].QuantityImage.QuantityText.Text
local Price = 10000
game:GetService("ReplicatedStorage").Events.SubmitTrade:FireServer(Item, Amount, Price)
wait(1)
